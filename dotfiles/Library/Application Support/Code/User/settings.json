{
    "cSpell.showStatus": false,
    "cSpell.userWords": [
        "buffer",
        "chunkhash",
        "codesplit",
        "contenthash",
        "deduped",
        "devtool",
        "devtools",
        "format",
        "gradlew",
        "hooks",
        "intelli",
        "j",
        "kapt",
        "lifecycle",
        "mixins",
        "multiclip",
        "netlify",
        "on",
        "postgre",
        "requery",
        "save",
        "singleline",
        "size",
        "splitted",
        "sql",
        "terraform",
        "treeshaking",
        "vetur"
    ],

    "docker.showExplorer": false,

    "editor.tabSize": 2,
    "editor.fontSize": 14,
    "editor.cursorStyle": "block",
    "editor.autoClosingBrackets": false,

    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue",
        "vue-html"
    ],

    "explorer.openEditors.visible": 0,

    "files.trimTrailingWhitespace": true,

    "markdown.extension.preview.autoShowPreviewToSide": true,
    "markdown.extension.quickStyling": true,

    "multiclip.bufferSize": 200,

    "terraform.formatOnSave": true,
    "terraform.formatVarsOnSave": true,
    "terraform.indexing": {
        "enabled": true,
        "indexerPath": "terraform-index",
        "liveIndexing": true,
        "delay": 500
    },

    "workbench.startupEditor": "newUntitledFile",
    "workbench.tips.enabled": false
}
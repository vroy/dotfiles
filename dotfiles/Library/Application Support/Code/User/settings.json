{
    "cSpell.showStatus": false,
    "cSpell.userWords": [
        "buffer",
        "chunkhash",
        "codesplit",
        "contenthash",
        "deduped",
        "devtool",
        "devtools",
        "hooks",
        "lifecycle",
        "mixins",
        "multiclip",
        "netlify",
        "singleline",
        "size",
        "splitted",
        "treeshaking",
        "vetur"
    ],

    "docker.showExplorer": false,

    "editor.tabSize": 2,
    "editor.fontSize": 14,
    "editor.cursorStyle": "block",
    "editor.autoClosingBrackets": false,

    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue",
        "vue-html"
    ],

    "explorer.openEditors.visible": 0,

    "files.trimTrailingWhitespace": true,

    "multiclip.bufferSize": 200,

    "workbench.startupEditor": "newUntitledFile",
    "workbench.tips.enabled": false
}